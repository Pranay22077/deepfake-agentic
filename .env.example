# E-Raksha Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
FRONTEND_PORT=3001

# Model Configuration
MODEL_PATH=./fixed_deepfake_model.pt

# Database Configuration (if using local database)
DATABASE_URL=postgresql://user:password@localhost:5432/eraksha

# Security
SECRET_KEY=your_secret_key_here
ALLOWED_ORIGINS=http://localhost:3001,http://localhost:3000

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Development/Production
ENVIRONMENT=development
DEBUG=false

# Optional: Kaggle API (for model training)
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_key